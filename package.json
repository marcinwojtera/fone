  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "lint": "run-p lint:* --continue-on-error",
    "lint:js": "eslint --ext .jsx,.js src",
    "start-client": "node build/bundle.js",
    "start": "rm -rf build && rm -rf public && webpack  --config webpack.server.js && webpack -p --config webpack.client.prod.js --mode production && npm run start-client",
    "webpack-prod": "rm -rf build && rm -rf public && webpack -p --progress --config webpack.client.prod.js --mode production",
    "dev": "npm-run-all --parallel dev:* ",
    "build-server": "webpack --config webpack.server.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "precommit": "pretty-quick --staged"
  },
  "author": "mla",
