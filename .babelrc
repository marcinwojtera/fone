{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties", "@babel/plugin-transform-runtime", "emotion"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types", "emotion"
      ]
    }
  }
}
